{
  "plugins": ["react", "react-hooks"],
  "extends": [
    "@jurajmatus/eslint-config-ts-neutral",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/react-in-jsx-scope": ["off"]
  }
}
